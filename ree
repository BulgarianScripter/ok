coroutine.wrap(AISHPR_fake_script)()
local function JYZV_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local UIXS = game:GetService('UserInputService')
	local dragToggle = nil
	local dragSpeed = 0.05
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(script.Parent, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	script.Parent.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = script.Parent.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIXS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(JYZV_fake_script)()
	game.StarterGui:SetCore("ChatMakeSystemMessage", {Text = "VELIHUB Are Loaded, Enjoy "..game:GetService("Players").LocalPlayer.Name.."."})
	wait(math.random(1.05,3.05))
	VeliHub.Enabled = true
end
