local function VRBQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local Toggle = true
	local UseToggle = false
	local StatusView = script.Parent.Parent.Frame
	local StrokeView = StatusView.UIStroke
	-------
	local TargetColor = script.Parent.Parent.Parent.Parent.Parent.Parent.UIColor
	
	script.Parent.MouseButton1Click:Connect(function()
		if Toggle == true then
			Toggle = false
			UseToggle = true
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{ImageColor3 = TargetColor.BackgroundColor3}):Play()
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Size = UDim2.new(0, 23,0, 23)}):Play()
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{ImageTransparency = 0}):Play()
			game:GetService("TweenService"):Create(StrokeView,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Color = TargetColor.BackgroundColor3}):Play()
			game:GetService("TweenService"):Create(StatusView.UIStroke,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Transparency = 1}):Play()
		else
			Toggle = true
			UseToggle = false
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{ImageColor3 = Color3.fromRGB(50,50,50)}):Play()
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Size = UDim2.new(0, 0,0, 0)}):Play()
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{ImageTransparency = 1}):Play()
			game:GetService("TweenService"):Create(StrokeView,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Color = Color3.fromRGB(50,50,50)}):Play()
			game:GetService("TweenService"):Create(StatusView.UIStroke,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Transparency = 0}):Play()
		end
	end)
	game:GetService("RunService").Stepped:Connect(function(v)
		if UseToggle == true then
			if game:GetService("Workspace"):FindFirstChild("Vehicles") then
				if game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name.."") then
					if game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name..""):FindFirstChild("Constraints") then
						for i,v in pairs(game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name..""):FindFirstChild("Constraints"):GetChildren()) do
							if v.ClassName == "HingeConstraint" then
								v.MotorMaxTorque = script.Parent.Parent.Parent.Parent.CarBoost.Holder.Input.Text
							end
						end
					end
				end
			end
			if game:GetService("Workspace"):FindFirstChild("Vehicles") then
				if game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name.."") then
					if game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name..""):FindFirstChild("Stats") then
						if game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name..""):FindFirstChild("Stats"):FindFirstChild("MaxSpeed") then
							game:GetService("Workspace"):FindFirstChild("Vehicles"):FindFirstChild(""..game:GetService("Players").LocalPlayer.Name..""):FindFirstChild("Stats"):FindFirstChild("MaxSpeed").Value = script.Parent.Parent.Parent.Parent.CarSpeed.Holder.Input.Text
						end
					end
				end
			end
			game:GetService("TweenService"):Create(StatusView.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{ImageColor3 = TargetColor.BackgroundColor3}):Play()
			game:GetService("TweenService"):Create(StrokeView,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Color = TargetColor.BackgroundColor3}):Play()
			--
		else
			--
		end
	end)
	
end
